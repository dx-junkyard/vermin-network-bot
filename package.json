{
  "name": "vermin-network-bot",
  "version": "1.0.0",
  "description": "A template LINE bot to report incidents of animal damage.",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "start:dev": "cross-env NODE_ENV=development node ./dist/api/index.js",
    "start": "node ./dist/api/index.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,ts,json}'",
    "prepare": "husky install"
  },
  "license": "MIT",
  "author": "FooQoo",
  "dependencies": {
    "@line/bot-sdk": "^7.3.0",
    "@prisma/client": "^4.11.0",
    "aws-sdk": "^2.1328.0",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "log4js": "^6.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/log4js": "^2.3.5",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "2.8.8",
    "prisma": "^4.10.1",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}